{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "parLocation": {
            "value": "westeurope"
        },
        "parResourceTags": {
            "value": {
                "Environment": "Development",
                "Description": "ACR for Bicep Modules"
            }
        },
        "parAcrSku": {
            "value": "Premium"
        },
        "parEnableSystemIdentity": {
            "value": true
        },
        "parEnableAdminUser": {
            "value": false
        },
        "parPublicNetworkAccess": {
            "value": "Enabled"
        },
        "parZoneRedundancy": {
            "value": "Disabled"
        },
        "parNetworkRuleSet": {
            "value": {
                "defaultAction": "Deny",
                "ipRules": [
                    {
                        "action": "Allow",
                        "value": ""
                    },
                    {
                        "action": "Allow",
                        "value": ""
                    }
                ],
                "virtualNetworkRules": [
                    {
                        "action": "Allow",
                        "id": ""
                    },
                    {
                        "action": "Allow",
                        "id": ""
                    }
                ]
            }
        },
        "parNetworkRuleByPassOptions": {
            "value": "None"
        },
        "parPolicies": {
            "value": {
                "RetentionPolicy": {
                    "status": "enabled",
                    "days": "30"
                }
            }
        },
        "parEnableDiagnostics": {
            "value": true
        },
        "parEnableDeleteLock": {
            "value": true
        },
        "parLogAnalyticsWorkspaceId": {
            "value": ""
        },
        "parDiagnosticStorageAccountId": {
            "value": ""
        }
    }
}